# VNPay Configuration
# Get these values from your VNPay merchant account
VITE_VNPAY_TMNCODE=your_terminal_id_here
VITE_VNPAY_HASHSECRET=your_secret_key_here

# Optional: Override default VNPay sandbox URL
# VITE_VNPAY_URL=https://sandbox.vnpayment.vn/paymentv2/vpcpay.html

# Optional: Override return URL (default is current domain + /payment-return)
# VITE_VNPAY_RETURNURL=https://yourdomain.com/payment-return

# Cloudflare Turnstile Configuration
VITE_CLOUDFLARE_TURNSTILE_SITE_KEY=your_site_key_here

# Telegram Configuration
# Get these from BotFather and your chat ID
VITE_TELEGRAM_BOT_TOKEN=your_bot_token_here
VITE_TELEGRAM_CHAT_ID=your_chat_id_here
